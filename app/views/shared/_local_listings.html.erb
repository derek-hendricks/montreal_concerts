<% @doc.css('table table tr td:nth-child('+column_num+')').each_with_index do |artist, index| %>
	<% if column_num != '2' %>
		<div class="row show-for-large-only">
	<% else %>
		<div class="row">
	<% end %>
		<div class="large-12 columns">
			<% if column_num == '2' %>
				<%= link_to "#{artist.content}", concert_path(artist.content) if index != 0 %>
			<% else %>
				<span class="dates-venues"> <%= artist.content if index != 0 %></span>
			<% end %>
		</div>
	</div>
<% end %>